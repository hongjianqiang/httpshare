{
  "name": "quickshare",
  "version": "1.2.0",
  "description": "一个类似Python的SimpleHTTPServer、http.server模块，但拥有上传和断点续传的功能",
  "main": "./lib/quickshare.min.js",
  "bin": "./bin.js",
  "scripts": {
    "dev-client": "cross-env TARGET=Client vue-cli-service serve",
    "dev-server": "cross-env TARGET=Server vue-cli-service build --target lib --formats umd-min --filename quickshare --watch src/server/main.ts",
    "build-client": "cross-env TARGET=Client vue-cli-service build --target app",
    "build-server": "cross-env TARGET=Server vue-cli-service build --target lib --formats umd-min --filename quickshare src/server/main.ts",
    "lint-client": "cross-env TARGET=Client vue-cli-service lint",
    "lint-server": "cross-env TARGET=Server vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hongjianqiang/quickshare.git"
  },
  "author": "HongJianQiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hongjianqiang/quickshare/issues"
  },
  "homepage": "https://github.com/hongjianqiang/quickshare#readme",
  "dependencies": {
    "core-js": "3.6.5",
    "vue": "2.6.12",
    "vue-class-component": "7.2.3",
    "vue-property-decorator": "8.4.2"
  },
  "devDependencies": {
    "@types/node": "^8.10.62",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-typescript": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/eslint-config-typescript": "5.0.2",
    "cross-env": "7.0.2",
    "eslint": "5.16.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "6.2.2",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.2",
    "typescript": "3.9.7",
    "vue-template-compiler": "2.6.12",
    "raw-loader": "4.0.1"
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 5.2.0"
  }
}
